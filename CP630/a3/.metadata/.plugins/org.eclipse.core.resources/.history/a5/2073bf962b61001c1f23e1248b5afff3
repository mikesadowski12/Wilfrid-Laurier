package stats.ejb;

public class SoccerStatsSummary implements java.io.Serializable {
	private int count; // number of elements
	private double min; // minimum element
	private double max; // maximum element
	private double mean; // mean
	private double std; // standard deviation

	public SoccerStatsSummary(int count, double min, double max, double mean, double std) {
		this.count = count;
		this.min = min;
		this.max = max;
		this.mean = mean;
		this.std = std;
	}
	
	public int getCount() {
		return this.count;
	}
	
	public double getMin() {
		return this.min;
	}
	
	public double getMax() {
		return this.max;
	}

	public double getMean() {
		return this.mean;
	}

	public double getSTD() {
		return this.std;
	}
}
